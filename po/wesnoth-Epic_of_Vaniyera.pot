msgstr ""

#. [unit]: type=Wild Elf Thunderblade, id=Dubhain
#: ./Epic_of_Vaniyera/scenarios/Epilogue.cfg:98
msgid "Dubhain"
msgstr ""

#. [unit]: type=Wild Elf Thunderblade, id=Ceallayach
#: ./Epic_of_Vaniyera/scenarios/Epilogue.cfg:105
